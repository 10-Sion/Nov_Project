<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://xmlns.jcp.org/xml/ns/javaee"
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_8_0.xsd">
version="4.0"
  <display-name>Nov_Project</display-name>
  	
  	<taglib>
        <taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
        <taglib-location>/WEB-INF/lib/jstl-1.2.jar</taglib-location>
    </taglib>
  	
	<servlet>
	    <servlet-name>bookmarkController</servlet-name>
	    <servlet-class>controller.BookmarkController</servlet-class>
	</servlet>
	
	<servlet-mapping>
	    <servlet-name>bookmarkController</servlet-name>
	    <url-pattern>/bookmarks</url-pattern>
	</servlet-mapping>
	
</web-app>